{
  "name": "deck-of-cards",
  "version": "1.0.0",
  "description": "Package that simulates actions with a deck of cards",
  "main": "./dist/deck.js",
  "scripts": {
    "test": "jest",
    "watch": "tsc --watch"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epndavis/deck-of-cards.git"
  },
  "keywords": [
    "cards",
    "typescript",
    "deck",
    "javascript"
  ],
  "author": "epndavis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/epndavis/deck-of-cards/issues"
  },
  "homepage": "https://github.com/epndavis/deck-of-cards#readme",
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "jest": "^26.0.1",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
